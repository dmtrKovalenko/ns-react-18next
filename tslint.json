{
  "extends": [
    "tslint-react",
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "object-literal-key-quotes": false,
    "import-name": false,
    "ban": false,
    "class-name": true,
    "ter-arrow-parens": false,
    "comment-format": [
      true,
      "check-space"
    ],
    "align": false,
    "curly": true,
    "no-unused-variable": true,
    "eofline": false,
    "forin": true,
    "no-empty-interface": false,
    "indent": [
      true,
      "spaces"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": false,
    "jsdoc-format": true,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "label-position": true,
    "max-line-length": [
      true,
      120
    ],
    "member-ordering": [
      true,
      "static-before-instance"
    ],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [
      true,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "only-arrow-functions": true,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "radix": true,
    "semicolon": [
      true,
      "always"
    ],
    "switch-default": true,
    "trailing-comma": [
      false
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ],
    "ordered-imports": false
  }
}
